{
  "name": "coach-app-baml-client",
  "version": "1.1.4",
  "description": "BAML-generated types and client for coach-app boundary contracts",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "coach-app",
    "baml",
    "contracts",
    "types"
  ],
  "author": "Coach App Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Frontierstrategies-ai/coach-app-contracts.git",
    "directory": "packages/typescript"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@boundaryml/baml": ">=0.68.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
